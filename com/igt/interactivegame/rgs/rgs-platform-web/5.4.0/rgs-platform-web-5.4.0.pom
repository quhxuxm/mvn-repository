                </dependency>
            </dependencies>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-dependency-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>unpack-dependency-resource-files</id>
                                <phase>process-classes</phase>
                                <goals>
                                    <goal>unpack</goal>
                                </goals>
                                <configuration>
                                    <artifactItems>
                                        <artifactItem>
                                            <groupId>com.igt.interactivegame.gle</groupId>
                                            <artifactId>gle-common</artifactId>
                                            <classifier>config-debug</classifier>
                                            <type>zip</type>
                                            <overWrite>true</overWrite>
                                            <outputDirectory>${project.build.directory}/generated/dependency-resources/gle-common-config-debug</outputDirectory>
                                            <includes>**</includes>
                                            <excludes>log4j.properties*, class-loader-config.xml*</excludes>
                                        </artifactItem>
                                        <artifactItem>
                                            <groupId>com.igt.platform.rgs.rng.war</groupId>
                                            <artifactId>rng-war</artifactId>
                                            <type>war</type>
                                            <overWrite>true</overWrite>
                                            <outputDirectory>${project.build.directory}/generated/dependency-resources/RNG</outputDirectory>
                                            <includes>WEB-INF/classes/RNG.properties,WEB-INF/classes/RNGLog4j.xml</includes>
                                        </artifactItem>
                                        <artifactItem>
                                            <groupId>com.igt.platform.rgs.rng.RandomRequestDocument</groupId>
                                            <artifactId>RandomRequestDocument</artifactId>
                                            <type>jar</type>
                                            <overWrite>true</overWrite>
                                            <outputDirectory>${project.build.directory}/generated/dependency-resources/RNG</outputDirectory>
                                            <includes>schemaorg_apache_xmlbeans/src/RandomRequestDocument.xsd</includes>
                                        </artifactItem>
                                    </artifactItems>
                                </configuration>
                            </execution>
                            <execution>
                                <id>prepare-python</id>
                                <goals>
                                    <goal>copy</goal>
                                </goals>
                                <phase>prepare-package</phase>
                                <configuration>
                                    <artifactItems>
                                        <artifactItem>
                                            <groupId>org.python</groupId>
                                            <artifactId>jython-standalone</artifactId>
                                            <outputDirectory>${basedir}/target/generated/jython</outputDirectory>
                                            <destFileName>jython-standalone.jar</destFileName>
                                        </artifactItem>
                                    </artifactItems>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>unfilter-build-process</id>
                                <phase>prepare-package</phase>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <configuration>
                                    <target>
                                        <exec executable="java">
                                            <arg value="-jar" />
                                            <arg
                                                value="${basedir}/target/generated/jython/jython-standalone.jar" />
                                            <arg
                                                value="${basedir}/src/main/ant/script/unfilter.py" />
                                            <arg value="${basedir}" />
                                        </exec>
                                    </target>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
