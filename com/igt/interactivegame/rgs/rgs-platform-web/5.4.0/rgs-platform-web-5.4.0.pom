                        </goals>
                        <configuration>
                            <target>
                                <ant antfile="${basedir}/src/main/ant/build.xml"
                                    target="tomcat-build-process">
                                </ant>
                            </target>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
    <profiles>
        <profile>
            <id>filter</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>com.igt.interactivegame.rgs</groupId>
                    <artifactId>rgs-platform-web-filtered</artifactId>
                    <version>${project.version}</version>
                </dependency>
            </dependencies>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-dependency-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>unpack-dependency-resource-files</id>
                                <phase>process-classes</phase>
                                <goals>
                                    <goal>unpack</goal>
                                </goals>
                                <configuration>
                                    <artifactItems>
                                        <artifactItem>
                                            <groupId>com.igt.interactivegame.gle</groupId>
                                            <artifactId>gle-common</artifactId>
                                            <classifier>config-release</classifier>
                                            <type>zip</type>
                                            <overWrite>true</overWrite>
                                            <outputDirectory>${project.build.directory}/generated/dependency-resources/gle-common-config-release</outputDirectory>
                                            <includes>**</includes>
                                            <excludes>class-loader-config.xml*</excludes>
                                        </artifactItem>
                                        <artifactItem>
                                            <groupId>com.igt.platform.rgs.rng.war</groupId>
                                            <artifactId>rng-war</artifactId>
                                            <type>war</type>
                                            <overWrite>true</overWrite>
                                            <outputDirectory>${project.build.directory}/generated/dependency-resources/RNG</outputDirectory>
                                            <includes>WEB-INF/classes/RNG.properties,WEB-INF/classes/RNGLog4j.xml</includes>
                                        </artifactItem>
                                        <artifactItem>
                                            <groupId>com.igt.platform.rgs.rng.RandomRequestDocument</groupId>
                                            <artifactId>RandomRequestDocument</artifactId>
                                            <type>jar</type>
                                            <overWrite>true</overWrite>
                                            <outputDirectory>${project.build.directory}/generated/dependency-resources/RNG</outputDirectory>
                                            <includes>schemaorg_apache_xmlbeans/src/RandomRequestDocument.xsd</includes>
                                        </artifactItem>
                                    </artifactItems>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
