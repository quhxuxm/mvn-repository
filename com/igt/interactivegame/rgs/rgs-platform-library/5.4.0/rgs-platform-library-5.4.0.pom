    </parent>
    <artifactId>rgs-platform-library</artifactId>
    <name>IGT Interactive Game - RGS Platform - Library</name>
    <description>IGT Interactive Game - RGS Platform - Library</description>
    <properties>
        <project.root.dir>${basedir}/../..</project.root.dir>
        <project.type.platform.app> true</project.type.platform.app>
    </properties>
    <dependencies>
        <dependency>
            <groupId>jotm</groupId>
